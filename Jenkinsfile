
pipeline{
    agent any
    stages{
        stage('Compile'){
            steps{
                echo "Compiling the script"
            }
        }
        stage('Build'){
            steps{
                echo "Building the script"
            }
        }
        stage('Test'){
            steps{
                echo "Deploying the script"
            }
        }
        stage('Release'){
            steps{
                echo "Releasing the script"
            }
        }
    }
}
